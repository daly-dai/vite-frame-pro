{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite-plugin-svgr/client"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@layout/*": [
        "src/layout/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@router/*": [
        "src/router/*"
      ],
      "@service/*": ["src/service/*"],
      "@types/*": ["src/types/*"],
      "@components/*": [
        "src/packages/components/*"
      ],
      "@plugins/*": [
        "src/plugins/*"
      ]
  },
  },
  "include": ["src/**/*", "./frame.config.ts"],
  "references": [{ "path": "./tsconfig.node.json" }],
  "exclude": ["node_modules", "dist","scripts", "build"]
}
